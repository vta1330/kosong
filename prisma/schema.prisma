generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model program_kerja {
  id Int @id @default(autoincrement())
  judul String @unique
  sub_judul String
  description Json
  kategori String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model kegiatan {
  id Int @id @default(autoincrement())
  judul String @unique
  description Json
  tanggal String
  kategori String
  image String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}